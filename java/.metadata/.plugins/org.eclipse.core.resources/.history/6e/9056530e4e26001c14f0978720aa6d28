package exam;

import java.util.Scanner;

public class Abc {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("첫번째 정수 : " );
		int a = sc.nextInt();
		System.out.println("두번째 정수 : " );
		int b = sc.nextInt();
		
		if(a>b) {
			System.out.println("큰수는"+ a);
		}else if(a<b) {
			System.out.println("큰수는 "+ b);
		}
		
	}
}
