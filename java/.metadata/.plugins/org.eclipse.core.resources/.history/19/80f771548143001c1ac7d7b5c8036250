package t06_daemon;

public class ThreadInfoExample {

	public static void main(String[] args) {
		AutoSaveThread t = new AutoSaveThread();
		t.setDaemon(true);
		t.setName("AutoSaveThread");
		t.start();
		
		Thread t1 = new Thread(()->{
			for(int i=0; i<1000000; i++);
		});
		t1.setName("t1");
		t1.start();
		
		
		
	}
}
